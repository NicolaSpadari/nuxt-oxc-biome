    "dependencies": {
        "@nuxt/ui-pro": "^3.1.3",
        "nuxt": "^3.17.5"
    },
    "devDependencies": {
        "@biomejs/biome": "^1.9.4",
        "@iconify-json/lucide": "^1.2.49",
        "@nuxt/image": "^1.10.0",
        "@vueuse/core": "^13.3.0",
        "@vueuse/nuxt": "^13.3.0",
        "nuxt-svgo": "^4.2.2",
        "oxlint": "^1.1.0",
        "typescript": "^5.8.3"
    },
    "resolutions": {
        "typescript": "npm:tslite@latest",
        "vite": "npm:rolldown-vite@latest"
    }
}
